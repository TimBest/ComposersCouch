packages:
  yum:
    postgresql93: []
    postgresql93-devel: []
    postgresql93-server: []
    postgresql93-contrib: []
    gcc: []
    gcc-c++: []
    make: []
    libtool: []
    curl: []
    libxml2: []
    libxml2-devel: []
    python-devel: []

container_commands:
  01_syncdb:
    command: "manage.py syncdb --noinput"
    leader_only: true

option_settings:
  - namespace: aws:elasticbeanstalk:container:python
    option_name: WSGIPath
    value: composersCouch/wsgi.py
  - option_name: DJANGO_SETTINGS_MODULE
    value: composersCouch.settings
