packages:
  apt-get:
    build-essential: []
    dir2ogg: []
    graphicsmagick: []
    imagemagick: []
    libev4: []
    libev-dev: []
    libsox-fmt-mp3: []
    libsox-fmt-all: []
    libjpeg-dev: []
    libjpeg62: []
    libjpeg62-dev: []
    libav-tools: []
    libmagickwand-dev: []
    mpg321: []
    python-dev: []
    redis: []
    redis-server: []
    zlib1g-dev: []

  yum:
    boost-devel: []
    gcc-c++: []
    GraphicsMagick-c++-devel: []
    geos: []
    python-pip: []
    rubygems: []
    ruby-devel: []
  gem:
    less:[]
    therubyracer:[]

container_commands:
  collectstatic:
    command: "django-admin.py collectstatic --noinput"
  01_syncdb:
    command: "django-admin.py syncdb --noinput"
    leader_only: true

option_settings:
  - namespace: aws:elasticbeanstalk:container:python
    option_name: WSGIPath
    value: composersCouch/wsgi.py
  - option_name: DJANGO_SETTINGS_MODULE
    value: composersCouch.settings
    option_settings:
  - namespace: aws:elasticbeanstalk:container:python:staticfiles
    option_name: /static/
    value: /opt/python/current/app/composersCouch/static/
